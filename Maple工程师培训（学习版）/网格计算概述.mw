<Group labelreference="L298" drawlabel="true">
<Input>
<Text-field style="Normal" layout="Normal"><Font style="Text">Define a self-contained procedure containing code that will be executed on each of the nodes.
</Font><Font style="Maple Input">sampleCode := proc()
   uses Grid;
   local thisNode, maxNodes, val, i, rply;
   thisNode := MyNode();
   maxNodes := NumNodes();
   print(&quot;PID=&quot;, kernelopts(pid));
   if thisNode &lt;&gt; 0 then
      # The other nodes: read input from the Master node
      val := Receive(0);
      val := val^thisNode;
      print(val);
      Send(0, val);
   else
      # This is the master node
      # Send something to other nodes
      for i from 1 to (maxNodes-1) do
         Send(i, (i+10));
      od;
      rply := 0;
      for i from 1 to (maxNodes-1) do
         val := Receive(i);
         print(val);
         rply := rply, val;
      od;
      return [rply];
   fi;
end:</Font></Text-field>
</Input>
