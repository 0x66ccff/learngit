<Text-field style="Line Printed Output" family="Serif" layout="Line Printed Output"><Font family="Serif">#include &lt;math.h&gt;

double f (int a, int b, double x)
{
  double x0;
  double x1;
  double v;
  double vp;
  x1 = x;
  x0 = x + 0.1e1;
  while (0.1e1 / 0.1000e4 &lt; fabs(x1 - x0))
  {
    v = cos((double) a * x1 + (double) b);
    vp = -(double) a * sin((double) a * x1 + (double) b);
    x0 = x1;
    x1 = x1 - v / vp;
  }
  return(x1);
}</Font></Text-field>
</Output>
