<Text-field prompt="&gt; " style="Maple Input" layout="Normal">collatzsteps := proc(n)
local steps, m;
  m := n;
  for steps while m &lt;&gt; 1 do
    if m mod 2 = 0 then
      m := m / 2;
    else
      m := 3 * m + 1;
    end if;
  end do;
  return steps - 1;
end proc;</Text-field>
</Input>
