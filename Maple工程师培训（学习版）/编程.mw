<Group view="code" labelreference="L21529" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">CellToValue := proc(XMLData)
local celldata;
  celldata := eval(XMLData, [_XML_Element = `[]`,
                             _XML_ElementType = (x -&gt; NULL),
                             _XML_Attribute = (x -&gt; NULL),
                             _XML_Entity = (x -&gt; NULL)]);
  celldata := ListTools:-Flatten(celldata);
  celldata := cat(op(map(XMLTools:-TextText, celldata)));
  if StringTools:-RegMatch(&quot;^[0-9.]+$&quot;, celldata) then
    return parse(celldata);
  else
    return celldata;
  end if;
end proc:</Text-field>
</Input>
</Group>
