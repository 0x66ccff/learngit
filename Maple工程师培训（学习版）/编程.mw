<Group view="code" labelreference="L22072" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
showstat(sin);</Text-field>
</Input>
<Output>
<Text-field style="Line Printed Output" layout="Line Printed Output">
sin := proc(x::algebraic)
local res, n, t, pull_out, keep_in, tmp;
   1   if nargs &lt;&gt; 1 then
   2     error &quot;expecting 1 argument, got %1&quot;, nargs
       elif type(x,'complex(float)') then
   3     return evalf(('sin')(x))
       elif type(x,'infinity') then
   4     if type(Re(x),'infinity') then
   5       res := `if`(type(Im(x),'undefined'),NumericTools:-ThrowUndefined(x),x*undefined)
         elif type(x,'imaginary') then
   6       res := x
         else
   7       res := infinity+infinity*I
         end if
       elif type(x,'undefined') then
   8     res := NumericTools:-ThrowUndefined(x,('preserve') = ('axes'))
       elif type(x,'SymbolicInfinity') and traperror(is(x,real)) = true then
   9     res := undefined
       elif type(x,'imaginary') or type(x,`*`) and member(true,map(type,{op(x)},'imaginary')) then
  10     res := I*sinh(-I*x)
       elif type(x,'complex(numeric)') then
  11     if csgn(x) &lt; 0 then
  12       res := -sin(-x)
         else
  13       res := ('sin')(x)
         end if
       elif type(x,`*`) and type(op(1,x),'complex(numeric)') and csgn(op(1,x)) &lt; 0 then
  14     res := -sin(-x)
       elif type(x,`*`) and type(x,'`&amp;*`(rational,identical(Pi))') then
  15     t := op(1,x);
  16     if t &lt; 1/2 then
  17       res := ('sin')(x)
         elif t &lt; 1 then
  18       res := sin((1-t)*Pi)
         elif t &lt; 2 then
  19       res := -sin((2-t)*Pi)
         else
  20       res := sin((t-2*iquo(trunc(t),2))*Pi)
         end if
       elif type(x,`*`) and select(type,[op(x)],('specfunc')('anything','csgn')) &lt;&gt; [] then
  21     pull_out, keep_in := selectremove(type,x,('specfunc')('anything','csgn'));
  22     res := pull_out*sin(keep_in)
       elif type(x,('specfunc')('anything','csgn')) then
  23     res := x*sin(1)
       elif type(x,`+`) and `tools/sign`(x) = -1 then
  24     res := -sin(-x)
       elif type(x,`+`) and has(x,Pi) then
  25     t := map(proc (x) if type(x/Pi,'rational') then x/Pi end if end proc,{op(x)});
  26     if nops(t) = 1 then
  27       t := op(t);
  28       if t &lt; 0 then
  29         res := sin(x-2*Pi*trunc(1/2*t)+2*Pi)
           elif t &lt; 1/2 then
  30         res := `sin/normal`(x)
           elif t &lt; 1 then
  31         res := cos(x-1/2*Pi)
           elif t &lt; 2 then
  32         res := -sin(x-Pi)
           else
  33         res := sin(x-2*Pi*trunc(1/2*t))
           end if
         else
  34       res := `sin/normal`(x)
         end if
       elif type(x,`*`) and member(Pi,[op(x)],'n') and Im(x) = 0 then
  35     t := subsop(n = 1,x);
  36     n := frac(t);
  37     if n = 0 then
  38       res := 0
         elif frac(1/2*t-1/4) = 0 then
  39       res := 1
         elif frac(1/2*t+1/4) = 0 then
  40       res := -1
         elif frac(t-1/2) = 0 then
  41       res := (-1)^(t-1/2)
         else
  42       res := `sin/normal`(x)
         end if
       elif type(x,'function') and nops(x) = 1 then
  43     n := op(0,x);
  44     t := op(1,x);
  45     if n = ('arcsin') then
  46       res := t
         elif n = ('arccos') then
  47       res := sqrt(1-t^2)
         elif n = ('arctan') then
  48       res := t/sqrt(1+t^2)
         elif n = ('arccsc') then
  49       res := 1/t
         elif n = ('arcsec') then
  50       res := sqrt(1-1/t^2)
         elif n = ('arccot') then
  51       res := 1/sqrt(1+t^2)
         else
  52       res := `sin/normal`(x)
         end if
       elif type(x,('specfunc')('anything','JacobiAM')) then
  53     res := JacobiSN(op(x))
       elif type(x,'arctan(algebraic,algebraic)') then
  54     res := op(1,x)/sqrt(op(1,x)^2+op(2,x)^2)
       else
  55     res := `sin/normal`(x);
  56     tmp := `trig/linear_in_Pi`(x);
  57     if tmp &lt;&gt; x then
  58       tmp := procname(tmp);
  59       if op(0,tmp) &lt;&gt; ('procname') then
  60         res := tmp;
  61         sin(x), sin(-x) := res, -res
           end if
         end if
       end if;
  62   sin(args) := res
end proc
