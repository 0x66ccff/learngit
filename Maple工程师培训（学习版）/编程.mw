Public Module CodeGenerationModule
  Public Function f( _
    ByVal a As Integer, _
    ByVal b As Integer, _
    ByVal x As Double) As Double
    Dim x0 As Double
    Dim x1 As Double
    Dim v As Double
    Dim vp As Double
    x1 = x
    x0 = x + 0.1E1
    Do While (0.1E1 / 0.1000E4 &lt; Abs(x1 - x0))
      v = Cos(CDbl(a) * x1 + CDbl(b))
      vp = -CDbl(a) * Sin(CDbl(a) * x1 + CDbl(b))
      x0 = x1
      x1 = x1 - v / vp
    Loop
    Return x1
  End Function
End Module</Font></Text-field>
</Output>
