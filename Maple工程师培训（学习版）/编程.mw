<Group view="code" labelreference="L21937" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">collatzsteps4 := proc(n :: posint,
                      base :: identical(1, 2, 4) := 1,
                      {show :: truefalse := false},
                      $)
local steps, m;
  m := n;
  for steps while m &lt;&gt; base do
    if show then
      lprint(m);
    end if;
    if m mod 2 = 0 then
      m := m / 2;
    else
      m := 3 * m + 1;
    end if;
  end do;
  return steps - 1;
end proc;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
</Output>
