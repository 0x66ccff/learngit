<Group view="code" labelreference="L22088" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
quicksort := proc(l :: list(numeric), $)
local less, greater, pivot, i;
  if l = [] then
    return l;
  end if;

  less := [];
  greater := [];
  pivot := l[1];
  for i from 2 to numelems(l) do
    if l[i] &lt; pivot then
      less := [op(less), l[i]];
    else
      greater := [op(greater), l[i]];
    end if;
  end do;

  return [op(quicksort(less)), pivot, op(quicksort(greater))];
end proc;</Text-field>
</Input>
</Group>
