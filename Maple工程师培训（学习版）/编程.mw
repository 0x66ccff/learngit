<Group view="code" labelreference="L21995" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">quicksort2 := proc(a :: Array(numeric),
                   l := 1,
                   u := numelems(a),
                   $)
local low_index, high_index;

  if u &lt;= l then
    return;
  end if;

  low_index := l + (rand() mod (u - l));
  if pivot_index &lt;&gt; l then
    a[l], a[low_index] := a[low_index], a[l];
  end if;

  high_index := u;
  low_index := l;

  while low_index &lt; high_index - 1 do
    if a[low_index + 1] &lt;= a[l] then
      low_index := low_index + 1;
    else
      high_index := high_index - 1;
      a[low_index + 1], a[high_index] := a[high_index], a[low_index + 1];
    end if;
  end do;

  procname(a, l, low_index);
  procname(a, high_index, u);
end proc:</Text-field>
</Input>
</Group>
